<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <!--Animation Select MenuItem-->
    <Storyboard x:Key="SelectAnimation_MenuItem">
        <ThicknessAnimation
            Storyboard.TargetProperty="BorderThickness"
            To="0 0 0 1"
            Duration="0:0:2"
            />
        <ColorAnimation
            Storyboard.TargetProperty="(BorderBrush).(SolidColorBrush.Color)"
            To="#557c93"
            Duration="0:0:1"
            />
    </Storyboard>
    <Storyboard x:Key="UnSelectAnimation_MenuItem">
        <ThicknessAnimation
            Storyboard.TargetProperty="BorderThickness"
            To="0 0 0 0"
            Duration="0:0:2"
            />
        <ColorAnimation
            Storyboard.TargetProperty="(BorderBrush).(SolidColorBrush.Color)"
            To="Transparent"
            Duration="0:0:1"
            />
    </Storyboard>

